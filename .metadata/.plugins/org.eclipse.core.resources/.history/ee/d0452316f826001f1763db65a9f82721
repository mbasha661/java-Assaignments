package moodleAssign;

public class Calculator {
	public static void main(String[] args)
	{
	Calculator calc = new Calculator();
	calc.add();
	calc.subtract();
	calc.multiply();
	calc.divide();
	}
	public void add()
	{
	System.out.println(10+20);
	}
	public void subtract()
	{
	System.out.println(10-20);
	}
	public void multiply()
	{
	System.out.println(10*20);
	}
	public void divide()
	{
	System.out.println(40/20);
	}

	}


